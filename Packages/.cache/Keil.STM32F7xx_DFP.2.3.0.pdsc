    <component Cclass="Device" Cgroup="Startup" Cversion="1.0.1" condition="STM32F7 CMSIS">
      <description>System Startup for STMicroelectronics STM32F7 Series</description>
      <RTE_Components_h>
        #define RTE_DEVICE_STARTUP_STM32F7XX    /* Device Startup for STM32F7 */
      </RTE_Components_h>
      <files>
        <!--  include folder -->
        <file category="include" name="Drivers/CMSIS/Device/ST/STM32F7xx/Include/"/>

        <!-- Flash Option Bytes templates -->
        <file category="source" condition="STM32F7_ARMCC" name="MDK/Device/Source/ARM/STM32F7xx_OPT.s" attr="template" select="Flash Option Bytes"/>
        <file category="source" condition="STM32F7_ARMCC" name="MDK/Device/Source/ARM/STM32F7xx_OTP.s" attr="template" select="Flash One-Time programmable Bytes"/>

        <!-- common device header file -->
        <file category="header"  name="Drivers/CMSIS/Device/ST/STM32F7xx/Include/stm32f7xx.h"/>

        <!-- startup files -->
        <!-- ARM Compiler Toolchain -->
        <file category="source" condition="STM32F745_ARMCC" name="Drivers/CMSIS/Device/ST/STM32F7xx/Source/Templates/arm/startup_stm32f745xx.s" attr="config" version="1.0.1"/>
        <file category="source" condition="STM32F746_ARMCC" name="Drivers/CMSIS/Device/ST/STM32F7xx/Source/Templates/arm/startup_stm32f746xx.s" attr="config" version="1.0.1"/>
        <file category="source" condition="STM32F756_ARMCC" name="Drivers/CMSIS/Device/ST/STM32F7xx/Source/Templates/arm/startup_stm32f756xx.s" attr="config" version="1.0.1"/>
        <!-- GCC Toolchain -->
        <file category="source" condition="STM32F745_GCC"   name="Drivers/CMSIS/Device/ST/STM32F7xx/Source/Templates/gcc/startup_stm32f745xx.s" attr="config" version="1.0.1"/>
        <file category="source" condition="STM32F746_GCC"   name="Drivers/CMSIS/Device/ST/STM32F7xx/Source/Templates/gcc/startup_stm32f746xx.s" attr="config" version="1.0.1"/>
        <file category="source" condition="STM32F756_GCC"   name="Drivers/CMSIS/Device/ST/STM32F7xx/Source/Templates/gcc/startup_stm32f756xx.s" attr="config" version="1.0.1"/>
        <!-- IAR Toolchain -->
        <file category="source" condition="STM32F745_IAR"   name="Drivers/CMSIS/Device/ST/STM32F7xx/Source/Templates/iar/startup_stm32f745xx.s" attr="config" version="1.0.1"/>
        <file category="source" condition="STM32F746_IAR"   name="Drivers/CMSIS/Device/ST/STM32F7xx/Source/Templates/iar/startup_stm32f746xx.s" attr="config" version="1.0.1"/>
