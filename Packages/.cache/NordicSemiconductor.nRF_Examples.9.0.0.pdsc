    <conditions>
        <condition id="ARM Compiler">
            <require Tcompiler="ARMCC"/>
        </condition>

        <condition id="IAR Compiler">
            <require Tcompiler="IAR"/>
        </condition>

        <condition id="GCC Compiler">
            <require Tcompiler="GCC"/>
        </condition>

        <condition id="nRF51 Series">
            <description>Nordic Semiconductor Device from nRF51 Series</description>
            <accept Dvendor="Nordic Semiconductor:54" Dname="nRF51822_xxAA"/>
            <accept Dvendor="Nordic Semiconductor:54" Dname="nRF51822_xxAB"/>
            <accept Dvendor="Nordic Semiconductor:54" Dname="nRF51822_xxAC"/>
            <accept Dvendor="Nordic Semiconductor:54" Dname="nRF51422_xxAA"/>
            <accept Dvendor="Nordic Semiconductor:54" Dname="nRF51422_xxAB"/>
            <accept Dvendor="Nordic Semiconductor:54" Dname="nRF51422_xxAC"/>
        </condition>
        
        <condition id="nRF52 Series">
            <description>Nordic Semiconductor Device from nRF52 Series</description>
            <accept Dvendor="Nordic Semiconductor:54" Dname="nRF52832_xxAA"/>
        </condition>

        <condition id="nRF51 Series CMSIS Device">
            <description>Nordic Semiconductor Device from nRF51 Series</description>
            <require Cclass ="CMSIS" Cgroup="CORE" Csub=""/>
            <accept condition="nRF51 Series"/>
            <accept condition="nRF52 Series"/>
        </condition>
        <condition id="generic_adc_Nrf51_0">
            <accept Cvendor="NordicSemiconductor" Csub="nrf51" Cgroup="Device" Cclass="None" />
        </condition>
        <condition id="generic_adc_Nrf51">
            <require condition="nRF51 Series CMSIS Device" />
            <require condition="generic_adc_Nrf51_0" />
            <deny Cvendor="NordicSemiconductor" Csub="nrf52" Cgroup="Device" Cclass="None" />
            <require Cversion="3.1.0" Cvendor="NordicSemiconductor" Cgroup="nrf_adc" Cclass="nRF_Drivers" />
        </condition>
        <condition id="generic_adc_Nrf52_0">
            <accept Cvendor="NordicSemiconductor" Csub="nrf52" Cgroup="Device" Cclass="None" />
        </condition>
        <condition id="generic_adc_Nrf52">
            <require condition="nRF51 Series CMSIS Device" />
            <require condition="generic_adc_Nrf52_0" />
            <deny Cvendor="NordicSemiconductor" Csub="nrf51" Cgroup="Device" Cclass="None" />
            <require Cvendor="NordicSemiconductor" Cgroup="nrf_drv_saadc" Cclass="nRF_Drivers" />
        </condition>

        <condition id="bsp_default">
            <require condition="nRF51 Series CMSIS Device" />
            <require Cversion="3.1.0" Cvendor="NordicSemiconductor" Cgroup="app_timer" Cclass="nRF_Libraries" />
            <require Cversion="3.1.0" Cvendor="NordicSemiconductor" Cgroup="app_button" Cclass="nRF_Libraries" />
            <require Cversion="3.1.0" Cvendor="NordicSemiconductor" Cgroup="app_error" Cclass="nRF_Libraries" />
            <require Cversion="3.1.0" Cvendor="NordicSemiconductor" Cgroup="nrf_error" Cclass="nRF_Drivers" />
            <require Cversion="3.1.0" Cvendor="NordicSemiconductor" Cgroup="app_util" Cclass="nRF_Libraries" />
            <deny Cvendor="NordicSemiconductor" Cgroup="unittest_framework" Cclass="Framework" />
        </condition>
        <condition id="bsp_Uart">
            <require condition="nRF51 Series CMSIS Device" />
            <require Cversion="3.1.0" Cvendor="NordicSemiconductor" Cgroup="app_timer" Cclass="nRF_Libraries" />
            <require Cversion="3.1.0" Cvendor="NordicSemiconductor" Cgroup="app_button" Cclass="nRF_Libraries" />
            <require Cversion="3.1.0" Cvendor="NordicSemiconductor" Cgroup="app_error" Cclass="nRF_Libraries" />
            <require Cversion="3.1.0" Cvendor="NordicSemiconductor" Cgroup="retarget" Cclass="nRF_Libraries" />
            <require Cversion="3.1.0" Cvendor="NordicSemiconductor" Cgroup="nrf_error" Cclass="nRF_Drivers" />
            <require Cversion="3.1.0" Cvendor="NordicSemiconductor" Cgroup="app_util" Cclass="nRF_Libraries" />
            <deny Cvendor="NordicSemiconductor" Cgroup="unittest_framework" Cclass="Framework" />
        </condition>
        <condition id="bsp_Simple_with_uart">
            <require condition="nRF51 Series CMSIS Device" />
            <require Cversion="3.1.0" Cvendor="NordicSemiconductor" Cgroup="app_error" Cclass="nRF_Libraries" />
            <require Cversion="3.1.0" Cvendor="NordicSemiconductor" Cgroup="app_uart" Cclass="nRF_Drivers" />
            <require Cversion="3.1.0" Cvendor="NordicSemiconductor" Cgroup="nrf_error" Cclass="nRF_Drivers" />
            <require Cversion="3.1.0" Cvendor="NordicSemiconductor" Cgroup="app_util" Cclass="nRF_Libraries" />
        </condition>
        <condition id="bsp_Defines">
            <require condition="nRF51 Series CMSIS Device" />
            <require Cversion="3.1.0" Cvendor="NordicSemiconductor" Cgroup="nrf_gpio" Cclass="nRF_Drivers" />
        </condition>

        <condition id="dfu_ant_flag">
            <require condition="nRF51 Series CMSIS Device" />
        </condition>


        <condition id="bsp_btn_ble">
            <require condition="nRF51 Series CMSIS Device" />
