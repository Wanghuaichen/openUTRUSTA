        <condition id="bsp_btn_ant">
            <require condition="nRF51 Series CMSIS Device" />
            <require Cversion="10.0.0" Cvendor="NordicSemiconductor" Cgroup="bsp" Cclass="Board Support" />
        </condition>

    </conditions>
